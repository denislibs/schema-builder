{
  "name": "pg-schema-builder",
  "version": "1.0.57",
  "description": "Инструмент для управления схемой базы данных PostgreSQL в Node.js",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run build:clean && npm run build:esm && npm run build:cjs && npm run build:types && npm run rename:files",
    "build:clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "rename:files": "node ../../scripts/rename-files.js",
    "lint": "eslint packages/main/src --ext .ts"
  },
  "author": "https://github.com/denislibs",
  "license": "MIT",
  "peerDependencies": {
    "pg": "^8.15.5"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "pg-mem": "^3.0.5",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vitest": "^3.1.1"
  }
}
