{
  "name": "@pg-schema-builder/migrator",
  "version": "1.0.0",
  "description": "Beautiful PostgreSQL migration tool",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.mjs",
      "require": "./dist/cjs/index.cjs",
      "types": "./dist/types/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:clean && npm run build:cjs && npm run build:esm && npm run build:types && npm run rename:files",
    "build:clean": "rimraf dist",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:types": "tsc -p tsconfig.types.json",
    "rename:files": "node ../../scripts/rename-files.js"
  },
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "pg": "^8.15.5"
  },
  "devDependencies": {
    "@types/boxen": "^3.0.5",
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^18.15.0",
    "@types/ora": "^3.2.0",
    "@types/pg": "^8.15.5",
    "rimraf": "^4.4.0",
    "typescript": "^5.0.2"
  }
}
