{
  "name": "pg-schema-builder",
  "version": "1.0.65",
  "description": "Инструмент для управления схемой базы данных PostgreSQL в Node.js",
  "main": "packages/main/cjs/index.cjs",
  "module": "packages/main/dist/esm/index.mjs",
  "types": "packages/main/dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./packages/main/dist/esm/index.mjs",
      "require": "./packages/main/dist/cjs/index.cjs",
      "types": "./packages/main/dist/types/index.d.ts"
    },
    "./cli": {
      "import": "./packages/cli/dist/esm/cli.mjs",
      "require": "./packages/cli/dist/cjs/cli.cjs",
      "types": "./packages/cli/dist/types/cli.d.ts"
    }
  },
  "files": [
    "packages/cli/dist",
    "packages/main/dist"
  ],
  "bin": {
    "pg-cli": "./packages/cli/dist/esm/cli.mjs"
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "build": "npm run build:schema && npm run build:cli",
    "build:schema": "cd packages/main && npm run build",
    "build:cli": "cd packages/cli && npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint packages/main/src --ext .ts"
  },
  "keywords": [
    "postgres",
    "postgresql",
    "schema",
    "migration",
    "database"
  ],
  "author": "https://github.com/denislibs",
  "license": "MIT",
  "dependencies": {
    "boxen": "^8.0.1",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "inquirer": "^9.2.12",
    "ora": "^8.0.1",
    "pg": "^8.15.5"
  },
  "devDependencies": {
    "@types/boxen": "^3.0.5",
    "@types/chalk": "^2.2.4",
    "@types/commander": "^2.12.5",
    "@types/inquirer": "^9.0.9",
    "@types/ora": "^3.2.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.0",
    "@types/pg": "^8.15.5",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitest/coverage-v8": "^3.1.1",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "pg-mem": "^3.0.5",
    "rimraf": "^4.4.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.2",
    "vitest": "^3.1.1"
  }
}
